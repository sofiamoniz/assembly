	.data
lista:	.word	4, 3 , -2, 1, 27, 45
string:	.asciiz	"O conteudo do Array Ã©: \n"
traco:	.asciiz	"   -   "
	.text
	.globl main
main:
	la	$a0,string
	li	$v0,4
	syscall
	
	addi	$t0,$0,0	#t0=i
	la	$t1,lista

for:
	sll	$t2,$t0,2
	addu	$t2,$t2,$t1	#t2=&lista[i]
	lb	$t3,0($t2)	#t3=lista[i]
	bge	$t3,6,end_for
	move	$a0,$t3
	li	$v0,1
	syscall
	la	$a0,traco
	li	$v0,4
	syscall
	addi	$t0,$t0,1	#i++
	j	for
end_for:
	li	$v0,10
	syscall
	